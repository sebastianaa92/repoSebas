conteos = 0
letras= ""

for conteo in range(1,101):
    if conteo % 3 == 0 and conteo % 5 == 0:
        letras = "FizzBuzz"
        print(letras)
    elif conteo % 3 == 0:
        letras = "Fizz"
        print(letras)
    elif conteo % 5 == 0:
        letras = "Buzz"
        print(letras)
    else: 
        conteos = conteo
        print(conteos)
