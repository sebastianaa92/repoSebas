student_scores = [123,150,176,116,160,118,120,130,116,155,157,167,164]

total_exam = sum(student_scores)
print(total_exam)

sum = 0
for score in student_scores:
    sum += score
print(sum)

print(max(student_scores))

max_coso = 0

for max_score in student_scores:
    if max_score > max_coso:
        max_coso = max_score
print(max_coso)